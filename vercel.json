{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://votre-backend-url.com/api/v1"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://votre-backend-url.com/api/$1"
    }
  ]
}
